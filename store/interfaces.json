{
  "search-result": {
    "allowed": [
      "not-found",
      "breadcrumb",
      "filter-navigator",
      "total-products",
      "order-by",
      "search-title",
      "rich-text"
    ],
    "required": ["gallery"],
    "component": "index",
    "render": "client"
  },
  "filter-navigator": {
    "component": "FilterNavigatorLegacy"
  },
  "filter-navigator.v1": {
    "component": "FilterNavigatorLegacy"
  },
  "filter-navigator.v2": {
    "component": "FilterNavigator",
    "allowed": ["shop-review-summary"]
  },
  "gallery": {
    "required": ["product-summary"],
    "component": "Gallery"
  },
  "not-found": {
    "allowed": ["shelf"],
    "component": "NotFoundSearch"
  },
  "order-by": {
    "component": "OrderBy"
  },
  "total-products": {
    "component": "TotalProducts"
  },
  "search-title": {
    "component": "SearchTitle"
  },
  "search-result-layout": {
    "allowed": [
      "search-result-layout.desktop",
      "search-result-layout.mobile",
      "search-not-found-layout"
    ],
    "component": "SearchResultLayout",
    "render": "lazy"
  },
  "search-result-layout.customQuery": {
    "allowed": [
      "search-result-layout.desktop",
      "search-result-layout.mobile",
      "search-not-found-layout"
    ],
    "component": "SearchResultLayoutCustomQuery",
    "render": "lazy"
  },
  "search-layout-switcher": {
    "component": "LayoutModeSwitcherFlexible"
  },
  "search-content": {
    "required": ["gallery", "not-found"],
    "component": "SearchContent"
  },
  "search-fetch-more": {
    "component": "FetchMore"
  },
  "search-fetch-previous": {
    "component": "FetchPrevious"
  },
  "search-result-layout.desktop": {
    "allowed": [
      "breadcrumb.search",
      "filter-navigator.v3",
      "total-products.v2",
      "order-by.v2",
      "search-title.v2",
      "rich-text",
      "info-card",
      "flex-layout",
      "tab-layout",
      "tab-list",
      "tab-content",
      "search-fetch-previous",
      "search-fetch-more",
      "search-content",
      "search-products-count-per-page",
      "search-blog-articles-preview",
      "search-blog-articles-list"
    ],
    "composition": "children",
    "component": "SearchResultFlexible"
  },
  "search-result-layout.mobile": {
    "allowed": [
      "breadcrumb.search",
      "filter-navigator.v3",
      "total-products.v2",
      "order-by.v2",
      "search-title.v2",
      "rich-text",
      "info-card",
      "flex-layout",
      "tab-layout",
      "search-layout-switcher",
      "search-fetch-previous",
      "search-fetch-more",
      "search-content",
      "search-products-count-per-page",
      "search-blog-articles-preview",
      "search-blog-articles-list"
    ],
    "composition": "children",
    "component": "SearchResultFlexibleMobile"
  },
  "search-not-found-layout": {
    "allowed": [
      "breadcrumb.search",
      "search-title.v2",
      "rich-text",
      "info-card",
      "flex-layout",
      "not-found",
      "search-bar"
    ],
    "composition": "children",
    "component": "NotFoundLayout"
  },
  "search-products-count-per-page": {
    "component": "ProductCountPerPage"
  },
  "order-by.v2": {
    "component": "OrderByFlexible"
  },
  "filter-navigator.v3": {
    "component": "FilterNavigatorFlexible"
  },
  "total-products.v2": {
    "component": "TotalProductsFlexible"
  },
  "search-title.v2": {
    "component": "SearchTitleFlexible"
  }
}
